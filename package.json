{
  "name": "hemtt",
  "displayName": "HEMTT",
  "description": "Use hemtt tool from your heavily themed VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "Reidond",
  "repository": {
    "type": "git",
    "url": "https://github.com/Reidond/vscode-hemtt"
  },
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "HEMTT",
      "properties": {
        "hemtt.path": {
          "type": "string",
          "default": "",
          "description": "Path to hemtt executable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  }
}
